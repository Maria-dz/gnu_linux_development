#: ../src/guess.c:61
#, c-format
msgid "Usage: ./guess [OPTIONS]\n"
msgstr "Использование: ./guess [ОПЦИИ]\n"

#: ../src/guess.c:62
#, c-format
msgid "Options:\n"
msgstr "Опции:\n"

#: ../src/guess.c:63
#, c-format
msgid "-r Use roman digits\n"
msgstr "-r Использовать римские цифры\n"

#: ../src/guess.c:64
#, c-format
msgid "--help See help information\n"
msgstr "--help Посмотреть вспомогательную информацию\n"

#: ../src/guess.c:89
#, c-format
msgid "Choose a number in range %s - %s\n"
msgstr "Выберите число %s - %s\n"

#: ../src/guess.c:92
#, c-format
msgid "Choose a number in range 1 - 100\n"
msgstr "Выберите число 1 -100\n"

#: ../src/guess.c:99
#, c-format
msgid "Is your number greater than %s? Answer 'yes' or 'no': "
msgstr "Ваше число больше %s? Ответьте 'да' или 'нет':"

#: ../src/guess.c:102
#, c-format
msgid "Is your number greater than %d? Answer 'yes' or 'no': "
msgstr "Ваше число больше %d? Ответьте 'да' или 'нет':"

#: ../src/guess.c:107
msgid "yes"
msgstr "да"

#: ../src/guess.c:109
msgid "no"
msgstr "нет"

#: ../src/guess.c:112
#, c-format
msgid "Incorrect format\n"
msgstr "Неверный формат\n"

#: ../src/guess.c:117
#, c-format
msgid "Your number is: %s\n"
msgstr "Ваше число: %s\n"

#: ../src/guess.c:120
#, c-format
msgid "Your number is: %d\n"
msgstr "Ваше число: %d\n"
